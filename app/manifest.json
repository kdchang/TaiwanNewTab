{
    "name": "TaiwanNewTab(台灣風景分頁)",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "分享台灣的美好風景",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/background.js"
        ]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
